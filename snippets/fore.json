			"Var",
			"\tcontrols: IPrxControls;",
			"\tctrl: IPrxControl;",
			"Begin",
			"\tcontrols := Report.Controls;",
			"\tFor Each ctrl In controls Do",
			"\t\t${1:// ...}",
			"\tEnd For;",
			"End"
		],
		"description": "Iterate report controls"
	},
	"Control: select element by Id": {
		"scope": "fore",
		"prefix": "control-select-by-id",
		"body": [
			"Var idx: Integer;",
			"Begin",
			"\tidx := Control.Selection.Dimension.Elements.FindById(\"${1:ID}\");",
			"\tControl.Selection.SelectElement(idx, True);",
			"End"
		],
		"description": "Select element in control by element Id"
	},
	"Date to DAYS string": {
		"scope": "fore",
		"prefix": "date-days",
		"body": [
			"Var dt: DateTime;",
			"Begin",
			"\tdt := DateTime.Today;",
			"\tctrl.SetValueC(String.Format(\"DAYS:{0}.{1}.{2}\",",
			"\t\tdt.Day < 10 ? \"0\" + String.Format(\"{0}\", dt.Day) : dt.Day,",
			"\t\tdt.Month < 10 ? \"0\" + String.Format(\"{0}\", dt.Month) : dt.Month,",
			"\t\tdt.Year));",
			"End"
		],
		"description": "Set control date in DAYS:dd.MM.yyyy format"
	},
	"SQL execute (DAL)": {
		"scope": "fore",
		"prefix": "sql-exec",
		"body": [
			"Var",
			"\tdb: IDatabaseInstance;",
			"\tcmd: IDalCommand;",
			"\tcur: IDalCursor;",
			"Begin",
			"\tdb := MetabaseClass.Active.ItemByIdNamespace(\"${1:APPLICATION_DB}\", ${2:BA_KEY}).Open(Null) As IDatabaseInstance;",
			"\tcmd := db.Connection.CreateCommand(\"\");",
			"\tcmd.SQL := \"${3:select 1}\";",
			"\tcur := cmd.CreateCursor;",
			"\tWhile Not cur.Eof Do",
			"\t\t${4:// val := cur.Fields.Item(0).Value;}",
			"\t\tcur.MoveNext;",
			"\tEnd While;",
			"\tcur.Close;",
			"\tcmd.Close;",
			"End"
		],
		"description": "Execute SQL via DAL"
	},
	"Iterate dim elements": {
		"scope": "fore",
		"prefix": "foreach-dim-elements",
		"body": [
			"Var",
			"\telements: IDimElements;",
			"\ti: Integer;",
			"Begin",
			"\telements := ${1:sel}.Dimension.Elements;",
			"\tFor i := 0 To elements.Count - 1 Do",
			"\t\t${2:// elements.Id(i); elements.Name(i)}",
			"\tEnd For;",
			"End"
		],
		"description": "Iterate elements of a dimension selection"
	},
	"For rows/cols in range": {
		"scope": "fore",
		"prefix": "for-range",
		"body": [
			"Var row, col: Integer;",
			"Begin",
			"\tFor row := ${1:range}.Top To ${1:range}.Bottom Do",
			"\t\tFor col := ${1:range}.Left To ${1:range}.Right Do",
			"\t\t\t${2:// tab.Cell(row, col).Value := ...}",
			"\t\tEnd For;",
			"\tEnd For;",
			"End"
		],
		"description": "Iterate over a rectangular range"
	},

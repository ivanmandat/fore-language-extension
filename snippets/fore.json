	"Metabase Open Object with Params": {
		"scope": "fore",
		"prefix": "mb-open",
		"body": [
			"Var",
			"\tmb: IMetabase;",
			"\tdesc: IMetabaseObjectDescriptor;",
			"\tparams: IMetabaseObjectParamValues;",
			"\tinst: IMetabaseObjectInstance;",
			"\ti: Integer;",
			"Begin",
			"\tmb := MetabaseClass.Active;",
			"\tdesc := mb.ItemByIdNamespace(\"${1:OBJECT_ID}\", ${2:BA_KEY});",
			"\tparams := desc.Params.CreateEmptyValues;",
			"\tFor i := 0 To params.Count - 1 Do",
			"\t\t// params.Item(i).Value := ...",
			"\tEnd For;",
			"\tinst := desc.Open(params);",
			"End"
		],
		"description": "Template: open metabase object with parameters"
	},

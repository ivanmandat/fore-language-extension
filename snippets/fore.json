	"EventsClass: set cube params for grid (data area)": {
		"scope": "fore",
		"prefix": "events-grid-params",
		"body": [
			"Public Class EventsClass: ReportEvents",
			"",
			"\t/// <summary>",
			"\t/// Set cube parameters for a data area (IEaxGrid) and refresh the grid.",
			"\t/// </summary>",
			"\tPublic Sub OnBeforeOpenReport(Report: IPrxReport; Var Cancel: Boolean);",
			"\tVar",
			"\t\tgrid: IEaxGrid;",
			"\t\tcubeInst: ICubeInstance;",
			"\t\tparams: IMetabaseObjectParamValues;",
			"\tBegin",
			"\t\t// Find your grid via DataArea → Slices → Views by View Id (e.g. AREA4)",
			"\t\tgrid := Report.DataArea.Slices.Item(${1:0}).Views.FindById(\"${2:AREA4}\") As IEaxGrid;",
			"\t\tIf grid Is Null Then",
			"\t\t\tExit Sub;",
			"\t\tEnd If;",
			"",
			"\t\t// Get current cube instance for the active pivot/grid",
			"\t\tcubeInst := CubeClass.CurrentCube;",
			"\t\tIf cubeInst Is Null Then",
			"\t\t\tExit Sub;",
			"\t\tEnd If;",
			"\t\t// Access parameter values of the current cube instance",
			"\t\tparams := cubeInst.ParamValues;",
			"\t\t// Examples: adjust to your parameter Ids",
			"\t\t// params.FindById(\"${3:P_DATE}\").Value := DateTime.Today;",
			"\t\t// params.FindById(\"${4:P_REGION}\").Value := \"${5:77}\";",
			"",
			"\t\t// Recalculate report to apply parameter changes",
			"\t\tPrxReport.ActiveReport.Recalc;",
			"\tEnd Sub OnBeforeOpenReport;",
			"",
			"End Class EventsClass;"
		],
		"description": "EventsClass template: set cube parameters for a sheet data area (IEaxGrid)"
	},

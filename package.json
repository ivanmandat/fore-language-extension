{
	"name": "foresight-fore-language",
	"displayName": "foresight FORE Language",
	"description": "Syntax highlighting and snippets for FORe (Foresight BI) desktop scripts.",
	"version": "0.0.5",
	"publisher": "ForeCode",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Programming Languages"
	],
	"icon": "icon.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ivanmandat/fore-language-extension"
	},
	"keywords": [
		"foresight",
		"fore",
		"bi",
		"pascal",
		"reports",
		"prx"
	],
	"contributes": {
		"languages": [
			{
				"id": "fore",
				"aliases": [
					"FORe",
					"fore"
				],
				"extensions": [
					".fore"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "fore",
				"scopeName": "source.fore",
				"path": "./syntaxes/fore.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "fore",
				"path": "./snippets/fore.json"
			}
		]
	}
}

